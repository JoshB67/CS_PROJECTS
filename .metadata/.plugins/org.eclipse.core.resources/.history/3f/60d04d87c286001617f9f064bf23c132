
public class DigPow {


	public static long digPow(int n, int p) {
		int results = 0;

		String num = String.valueOf(n);
		for (int i = 0; i < num.length(); i++) {
			char c = num.charAt(i);
            int x = Character.getNumericValue(c);
			results += x^(p);
			p++;            
		}

//		int k = 0;
//
//		while( k < results ) {
//			k++;
//			if(k*n == results){
//				return k;
//			}
//	}
		return results;
	}


	public static void main(String[] args) {
			System.out.println(DigPow.digPow(695, 2));
	}
}