
public class DigPow {


	public static long digPow(int n, int p) {
		int results = 0;

		while (n > 0) {
		  int d = n / 10;
		  int k = n - d * 10;
		  n = d;
		  results += k^(p);
		  p++;   
		}
		

//		int k = 0;
//
//		while( k < results ) {
//			k++;
//			if(k*n == results){
//				return k;
//			}
//	}
		return results;
	}


	public static void main(String[] args) {
			System.out.println(DigPow.digPow(695, 2));
	}
}